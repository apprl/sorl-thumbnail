#!/bin/bash

if [[ -z $PROJDIR ]]; then
    PROJDIR=`pwd`
fi

PROJNAME='apparelrow'                           # Django project name

export PYTHONPATH="$PROJDIR/$PROJNAME:$PROJDIR"
export DJANGO_SETTINGS_MODULE=settings

if [[ -e $PROJDIR/bin/activate ]]; then
    . $PROJDIR/bin/activate
fi
cd $PROJDIR/lib

python runner.py $@
